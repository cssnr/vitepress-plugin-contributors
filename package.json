  "description": "VitePress Plugin to easily display GitHub contributors and automatically updated on every install/deploy.",
  "homepage": "https://github.com/cssnr/vitepress-plugin-contributors",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cssnr/vitepress-plugin-contributors.git"
